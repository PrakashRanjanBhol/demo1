export async function customWorkflowAuthMiddleware(  
	req: AuthenticatedRequest,  
	res: Response,  
	next: NextFunction,  
) {  
	// Use 'name' instead of 'workflowId' based on route definition  
	const workflowId = req.params.name; // or req.params.id depending on route  
	  
	if (!workflowId) {  
		return res.status(400).json({ error: 'Workflow ID missing' });  
	}  
	  
	// Check if user is already authenticated  
	if (req.user) {  
		return next();  
	}  
