 async createAgent(payload: any): Promise<any> {
    return await firstValueFrom(
      this.http.post<any>('https://api.example.com/agent', payload)
    );
  }

async saveAgent() {
  try {
    const response = await this.apiService.createAgent({
      name: 'AI Agent',
      type: 'autonomous'
    });

    console.log('Success:', response);
  } catch (error) {
    console.error('API Error:', error);
  }
}
