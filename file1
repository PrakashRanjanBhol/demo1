  const apiResponse = await fetch('https://your-api-endpoint.com/process-credentials', {  
      method: 'POST',  
      headers: {  
        'Content-Type': 'application/json',  
        // Add any other headers needed  
      },  
      body: JSON.stringify({  
        userId: data.userId,  
        token: data.token  
      })  
    });  
      
    if (!apiResponse.ok) {  
      throw new Error(`API request failed: ${apiResponse.status}`);  
    }  
      
    const credentials = await apiResponse.json();  
      
    // Resolve with the API response  
    resolve(credentials);  
