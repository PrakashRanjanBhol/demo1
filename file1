packages/frontend/editor-ui/src/features/core/auth/views/SessionExpiredView.vue


<template>  
  <div :class="$style.container">  
    <N8nLogo size="large" />  
    <div :class="$style.textContainer">  
      <N8nText size="large" tag="h1">Session Expired</N8nText>  
      <N8nText size="medium" tag="p">  
        Your session has expired. Please sign in again to continue.  
      </N8nText>  
    </div>  
    <div :class="$style.formContainer">  
      <N8nButton  
        label="Sign In"  
        type="primary"  
        size="large"  
        @click="goToSignIn"  
      />  
    </div>  
  </div>  
</template>  
  
<script lang="ts">  
import { defineComponent } from 'vue';  
import { useRouter } from 'vue-router';  
import { VIEWS } from '@/constants';  
  
export default defineComponent({  
  name: 'SessionExpiredView',  
  setup() {  
    const router = useRouter();  
      
    const goToSignIn = () => {  
      router.push({ name: VIEWS.SIGNIN });  
    };  
      
    return {  
      goToSignIn,  
    };  
  },  
});  
</script>  
  
