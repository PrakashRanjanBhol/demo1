// Add these trackBy methods to the ChunkedJsonParserComponent class

// TrackBy functions for optimal rendering performance
public trackByFileId(index: number, item: ProcessedFileItem): string {
  return item.id;
}

public trackByLineIndex(index: number, item: DiffLine): number {
  return index;
}

public trackByFileName(index: number, item: CompletedFileInfo): string {
  return item.fileName + item.tool;
}
