private updateFavoriteRepoKeysSet(): void {
  console.log('Updating Set, current favorites:', this.favoriteRepositories); // ✅ Debug
  
  this.favoriteRepoKeysSet.clear();
  
  Object.values(this.favoriteRepositories).forEach(repos => {
    repos.forEach(repo => {
      console.log('Adding to Set:', repo.repo_key); // ✅ Debug
      this.favoriteRepoKeysSet.add(repo.repo_key);
    });
  });
  
  console.log('Set size:', this.favoriteRepoKeysSet.size); // ✅ Debug
  console.log('Set contents:', Array.from(this.favoriteRepoKeysSet)); // ✅ Debug
}
