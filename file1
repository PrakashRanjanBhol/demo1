catch (error) {  
  if (error.response?.status === 401 && endpoint !== '/login') {  
    try {  
      // Fetch credentials from external API  
      const credentialResponse = await fetch('https://your-external-api.com/get-credentials', {  
        method: 'GET',  
        headers: {  
          'Content-Type': 'application/json',  
          // Add any auth headers needed for your external API  
        }  
      });  
        
      if (!credentialResponse.ok) {  
        throw new Error('Failed to fetch credentials from external API');  
      }  
        
      const credentials = await credentialResponse.json();  
        
      // Login with fetched credentials  
      await request({  
        method: 'POST',  
        baseURL,  
        endpoint: '/login',  
        data: {  
          emailOrLdapLoginId: credentials.email,  
          password: credentials.password  
        }  
      });  
        
      // Retry original request after successful login  
      const response = await axios.request(options);  
      return response.data;  
    } catch (credentialError) {  
      console.error('Auto-login failed:', credentialError);  
      // Continue with original 401 error if credential fetch fails  
    }  
  }  
