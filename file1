// Define a dynamic color palette
const palette = [
  '#91cc75', '#fac858', '#ee6666',
  '#5470c6', '#73c0de', '#3ba272',
  '#fc8452', '#9a60b4', '#ea7ccc'
];

// Extract unique d_types
const uniqueTypes = [...new Set(response.map(item => item.d_type))];

// Create dynamic colorMap
const colorMap: { [key: string]: string } = {};
uniqueTypes.forEach((type, index) => {
  colorMap[type] = palette[index % palette.length];
});
